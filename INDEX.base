views:
  - type: table
    name: Table
    filters:
      and:
        - file.folder != "/"
        - and:
            - file.name != "_OGLEDNA"
        - and:
            - file.name != "_OGLEDNA PITANJA"
        - and:
            - file.name != "_DIJAGRAMI"
    groupBy:
      property: file.folder
      direction: ASC
    order:
      - file.folder
      - file.name
    sort:
      - property: file.folder
        direction: ASC
